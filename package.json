{
  "name": "@mediafishio/react-bootstrap-forms",
  "version": "1.0.0",
  "description": "Advanced Form Components built on React Bootstrap.",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "start": "babel src --watch --out-dir build --extensions .ts,.tsx --source-maps inline --copy-files",
    "tsc:watch": "tsc --watch --emitDeclarationOnly",
    "build": "babel src --out-dir build --extensions .ts,.tsx --source-maps inline --copy-files && tsc --declaration --emitDeclarationOnly",
    "clean": "rimraf build",
    "npm:install": "npm install --legacy-peer-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hjrdave/React.Bootstrap.Forms.git"
  },
  "author": "David A. Sanders",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hjrdave/React.Bootstrap.Forms/issues"
  },
  "homepage": "https://github.com/hjrdave/React.Bootstrap.Forms#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/uniqid": "^5.3.2",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.5.1",
    "chai": "^4.1.2",
    "cross-env": "^5.2.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "mocha": "^6.1.3",
    "nyc": "^13.3.0",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.4",
    "vsts": "^2.1.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@types/react-dropzone": "^5.1.0",
    "react-bootstrap": "^2.7.0",
    "react-cork": "^1.0.0-alpha.36",
    "react-day-picker": "^8.6.0",
    "react-dropzone": "^12.0.4",
    "react-dropzone-uploader": "^2.11.0"
  }
}
